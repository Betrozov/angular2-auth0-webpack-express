{
  "name": "angular2-webpack-auth0-todo",
  "version": "0.0.0",
  "description": "Angular2 webpack auth0 todo",
  "author": "Yura Betrozov <betrozov91@gmail.com>",
  "homepage": "https://github.com/Betrozov/angular2-webpack",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "clean": "rimraf node_modules doc typings && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm dev",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "dev": "npm run server",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.0",
    "auth0": "^0.8.2",
    "body-parser": "^1.14.2",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "core-js": "^1.2.6",
    "es6-shim": "^0.34.1",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "mongodb": "^2.1.4",
    "mongoose": "^4.3.6",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.1",
    "pre-commit": "^1.1.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "static-favicon": "^1.0.2",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "raw-loader": "0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "typings": "^0.3.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
